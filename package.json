{
  "name": "json3d4aframe",
  "version": "2.0.3",
  "description": "JSON Editor for creating 3D models in AFrame and AR.js - tool is support tool for the Wikiversity learning resource for 3D modelling",
  "main": "src/main.js",
  "exportvar": "JSON3D4Aframe",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "node ./build4docs.js;doctoc README.md;node ./src/bin4build/create_index_n.js;node ./version4index.js;node ./version4readme.js",
    "doctoc": "doctoc ./README.md",
    "version4index": "node ./version4index.js",
    "test": "node src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niebert/JSON3D4Aframe.git"
  },
  "keywords": [
    "JSON",
    "Editor",
    "3D",
    "modelling",
    "AFrame",
    "AR.js",
    "geolocation",
    "marker",
    "based",
    "AR"
  ],
  "author": "Engelbert Niehaus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/niebert/JSON3D4Aframe/issues"
  },
  "homepage": "https://github.com/niebert/JSON3D4Aframe#readme",
  "devDependencies": {
    "build4code": "^0.3.31",
    "walker4folder": "^1.0.4"
  }
}
